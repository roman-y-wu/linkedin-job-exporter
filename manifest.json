{
  "manifest_version": 3,
  "name": "LinkedIn Job Exporter",
  "version": "1.0.0",
  "description": "Export LinkedIn job descriptions to PDF with one click",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs/*"],
      "js": ["lib/html2pdf.bundle.min.js", "content.js"],
      "css": ["styles.css"]
    }
  ]}
